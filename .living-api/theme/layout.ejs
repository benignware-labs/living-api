<html>
  <head>
    <script src="theme.js"></script>
    <title><%= config.title %></title>
  </head>
  <body class="ld-Frame">
    <header class="ld-Frame-header">
      <%- include('partials/navbar', { items: tree.items, config }); %>
    </header>
    <main class="ld-Frame-main">
      <aside id="ld--Frame-sidebar" class="ld--Frame-sidebar" data-turbolinks-permanent>
        <%- include('partials/menu', { items: config.navigation.items, config, level: 0 }); %>
      </aside>
      <div class="ld-Container">
        <article>
          <%- content %>
        </article>
      </div>
    </main>
  </body>
</html>
